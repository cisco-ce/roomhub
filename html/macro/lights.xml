<Extensions>
  <Version>1.8</Version>
  <Panel>
    <Order>1</Order>
    <PanelId>lights</PanelId>
    <Origin>local</Origin>
    <Location>HomeScreen</Location>
    <Icon>Lightbulb</Icon>
    <Color>#CF7900</Color>
    <Name>Lights</Name>
    <ActivityType>Custom</ActivityType>
    <Page>
      <Name>Lights</Name>
      <Row>
        <Name>Row</Name>
        <Widget>
          <WidgetId>lights-preset</WidgetId>
          <Type>GroupButton</Type>
          <Options>size=4</Options>
          <ValueSpace>
            <Value>
              <Key>0</Key>
              <Name>Off</Name>
            </Value>
            <Value>
              <Key>25</Key>
              <Name>25%</Name>
            </Value>
            <Value>
              <Key>50</Key>
              <Name>50%</Name>
            </Value>
            <Value>
              <Key>100</Key>
              <Name>Full</Name>
            </Value>
          </ValueSpace>
        </Widget>
      </Row>
      <Options>hideRowNames=1</Options>
    </Page>
  </Panel>
</Extensions>
